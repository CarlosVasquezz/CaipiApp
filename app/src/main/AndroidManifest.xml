        </activity>

        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ChildDataActivity" />
        <activity android:name=".ChildInfoActivity" />
        <activity android:name=".PlayschoolActivity" />
